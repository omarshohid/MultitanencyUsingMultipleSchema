<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    <link href="~/Content/animate.css" rel="stylesheet" />
    <script src="~/Scripts/angular.min.js"></script>
    @Scripts.Render("~/bundles/modernizr")

</head>
<body ng-app="myApp">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("Job List", "Index", "Home")</li>
                    <li>@Html.ActionLink("Create Job", "Create", "Home")</li>
                    <li>@Html.ActionLink("Create Tenant", "CreateTanent", "Home")</li>
                    <li>@Html.ActionLink("Create Tenant", "CreateTanent", "Home")</li>*@
                    <li style="list-style: none"><a href="#/JobList">Job List</a></li>
                    <li style="list-style: none"><a href="#/CreateJob">Create Job</a></li>
                    <li style="list-style: none"><a href="#/CreateTenant">Create Tenant</a></li>
                   
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content" style="padding-top:10px">
        <ng-view class="reveal-animation">
            @RenderBody()
        </ng-view>
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-2.1.1.js"></script>
    <script src="~/Scripts/angular-route.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0rc1/angular-route.min.js"></script>
    <script src="~/Scripts/angular-animate.js"></script>
    <script src="~/Scripts/ui-router.js"></script>
    <script src="~/Scripts/Route.js"></script>

    <script src="~/Scripts/JobController.js"></script>
    <script src="~/Scripts/TenantController.js"></script>
    @*@Scripts.Render("~/bundles/jquery")*@
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)

</body>
</html>
